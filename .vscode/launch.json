{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: FSL_SAGE_main",
            "type": "debugpy",
            "request": "launch",
            "cwd"   : "${workspaceFolder}/src",
            //"program": "${workspaceFolder}/src/FSL_SAGE_main.py",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--dataset", "cifar",
                "-seed", "200", 
                "-K", "3",
                "-L", "1",
                "-U", "3",
                "-B", "100",
                "-E", "1",
                "-batch_round", "5",
                "--server-lr", "0.001",
                "--client-lr", "0.001",
                "--round", "5",
                "--iid", "--gpu",
                //"--save"
            ]           
        }
    ]
}